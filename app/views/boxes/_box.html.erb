<% if box.away_team_coord == "A" %>
  <div class='dtable'>
    <div id='bottom_wrapper' class='drow'>
      <div class='box-cell dcell grid-letters home-letters'><h3><%= box.home_team_num %></h3></div>
<% end %>
    <% if box.user_id == nil %>
      <div class='box-cell dcell'>
          <%= button_tag(type: 'button', class: "box-button", id: "#{box.home_team_coord}#{box.away_team_coord}") do %>
            <%= content_tag(:i,"", class: "fa fa-th-large fa-fw") %>
        <% end %>
      </div>
    <% elsif box.user_id == @current_user.id %>
      <div class='box-cell dcell'>
          <%= button_tag(type: 'button', class: "box-button unpick", id: "#{box.home_team_coord}#{box.away_team_coord}") do %>
            <%= "#{@current_user.name}" %>
        <% end %>
      </div>
    <% else %>
      <div class='box-cell dcell'>
          <%= button_tag(type: 'button', class: "box-button", id: "#{box.home_team_coord}#{box.away_team_coord}") do %>
            <%= box.user.name %>
        <% end %>
      </div>
    <% end %>

<% if box.away_team_coord == "J" %>
    </div>
  </div>
<% end %>

